  "license": "MIT",
  "bugs": {
    "url": "https://github.com/qwghlm/sceptred/issues"
  },
  "homepage": "https://github.com/qwghlm/sceptred#readme",
  "devDependencies": {
    "@types/jest": "^22.1.4",
    "@types/modernizr": "^3.5.1",
    "@types/stats": "^0.16.30",
    "@types/three": "^0.89.10",
    "babel-core": "^6.26.0",
    "babel-jest": "^22.4.0",
    "babel-loader": "^7.1.2",
    "babel-preset-env": "^1.6.1",
    "css-loader": "^0.28.9",
    "extract-text-webpack-plugin": "^3.0.2",
