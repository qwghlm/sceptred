{
  "name": "sceptred",
  "version": "0.0.2",
  "description": "A project to model Great Britain in 3D",
  "main": "js/index.js",
  "scripts": {
    "dist": "webpack -p",
    "serve": "cd server && fresh",
    "watch": "webpack --watch",
    "test:js": "jest",
    "test:go": "go test ./server -coverprofile=./server/cover.out -tags test",
    "test": "npm run test:js && npm run test:go",
    "coverage": "open ./client/coverage/index.html; go tool cover -html=./server/cover.out"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/qwghlm/sceptred.git"
  },
